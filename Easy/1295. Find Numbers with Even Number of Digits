class Solution {
    public int findNumbers(int[] nums) {
        int count=0;
        for(int i=0;i<nums.length;i++)
        {
            if(check(nums[i])==true)
            {
                count++;
            }
        }
        return count;
    }
    public boolean check(int n)
    {
        int c=0;
        while(n>0)
        {
            c++;
            n=n/10;
        }
        if(c%2==0)
        return true;
        return false;
    }
}
